@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov-o/> .
@prefix ext:  <http://www.example.com/crime#> .
@prefix    :  <http://www.example.com/crimeFile#> .

:bob
    a ext:Journalist .

:charles
    a ext:Journalist .

:david
    a ext:Journalist .

:edith
    a ext:Journalist .

:alice
    a ext:Journalist .

:e1
    a ext:CrimeFile ;
    ext:hasFileContent ""^^xsd:string ;
    ext:hasFilePath <file:///share/crime.txt>;
    prov:wasGeneratedBy :pe0 .

:e2
    a ext:CrimeFile ;
    ext:hasFileContent "There was a lot of crime in London last month."^^xsd:string ;
    ext:hasFilePath <file:///share/crime.txt>;
    prov:wasDerivedFrom :e1 ;
    prov:wasGeneratedBy :pe1 .

:e3
    a ext:CrimeFile ;
    ext:hasFileContent "There was a lot of crime in London and New-York last month."^^xsd:string ;
    ext:hasFilePath <file:///share/crime.txt>;
    prov:wasDerivedFrom :e2 ;
    prov:wasGeneratedBy :pe3 .

:e4
    a ext:CrimeFile ;
    ext:hasFileContent "There was a lot of crime in London last month."^^xsd:string ;
    prov:wasDerivedFrom :e2 ;
    prov:wasGeneratedBy :pe2 .

:e5
    a ext:CrimeFile ;
    ext:hasFileContent "There was a lot of crime in London and New-York last month."^^xsd:string ;
    prov:wasDerivedFrom :e3 ;
    prov:wasGeneratedBy :pe4 .

:e6
    a ext:CrimeFile ;
    prov:wasDerivedFrom :e3 ;
    prov:wasGeneratedBy :pe5 .

:pe0
    a ext:FileCreation ;
    prov:wasControlledBy :alice .

:pe1
    a ext:FileAppending ;
    prov:used :e1 ;
    prov:wasControlledBy :bob .

:pe2
    a ext:Emailing ;
    prov:used :e2 ;
    prov:wasControlledBy :charles .

:pe3
    a ext:FileEditing ;
    prov:used :e2 ;
    prov:wasControlledBy :david .

:pe4
    a ext:Emailing ;
    prov:used :e3 ;
    prov:wasControlledBy :edith .

:pe5
    a ext:SpellChecking ;
    prov:used :e3 .

