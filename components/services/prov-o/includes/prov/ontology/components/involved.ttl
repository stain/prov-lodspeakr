@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov-o/> .

prov:involved
   a owl:ObjectProperty;
   rdfs:comment "A generic involvement between Entities and ProcessExecutions (and vice versa); a superproperty of ProcessExecutions' :used, :hadParticipant, :wasControlledBy, and Entities' :wasGeneratedBy";
   rdfs:domain [ owl:unionOf ( prov:Entity prov:ProcessExecution ) ];
   rdfs:range  [ owl:unionOf ( prov:Entity prov:ProcessExecution ) ];
.

prov:used            rdfs:subPropertyOf prov:involved .
prov:hadParticipant  rdfs:subPropertyOf prov:involved .
prov:wasControlledBy rdfs:subPropertyOf prov:involved .
prov:wasGeneratedBy  rdfs:subPropertyOf prov:involved .
