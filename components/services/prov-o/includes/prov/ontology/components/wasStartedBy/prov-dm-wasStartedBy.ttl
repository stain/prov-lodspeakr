@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix time:    <http://www.w3.org/2006/time#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix skos:    <http://www.w3.org/2008/05/skos#> .
@prefix prov:    <http://www.w3.org/ns/prov-o/> .
@prefix :        <#> .

:id
   rdfs:seeAlso <http://dvcs.w3.org/hg/prov/raw-file/f8c191710aad/model/ProvenanceModel.html#record-OrderingOfActivities>;
   dcterms:description "wasStartedBy(a2,a1)";
   dcterms:description "an optional identifier id;";
.

:a2
   dcterms:description "refers to an activity record identified by a2, representing the started activity;";
   a prov:Activity;
   prov:wasStartedBy :a1;
.

:a1
   dcterms:description "refers to an activity record identified by a1, representing the activity that started a1;";
   a prov:Activity;
   prov:started        :a2; # analogous to how prov:generated is used as the inverse of prov:wasGeneratedBy.
   prov:qualifiedStart :id; # this is the qualified parallel of prov:started.
.

:id
   a prov:Start;
   prov:qualifiedActivity :a2;

   dcterms:description "an optional set attrs of attribute-value pairs to further describe this record.";
   :third_party :attributes_here;
.
