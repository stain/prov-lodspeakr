@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix time:    <http://www.w3.org/2006/time#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix skos:    <http://www.w3.org/2008/05/skos#> .
@prefix prov:    <http://www.w3.org/ns/prov-o/> .
@prefix :        <#> .

:a1 a prov:Activity .

:a2
   rdfs:seeAlso <http://dvcs.w3.org/hg/prov/raw-file/f8c191710aad/model/ProvenanceModel.html#record-OrderingOfActivities>;
   dcterms:description "Given two activity records identified by a1 and a2, the record wasStartedBy(a2,a1) holds if and only if there exist an entity record identified by e and some attributes gAttr and sAttr, such that wasGeneratedBy(e,a1,gAttr) and wasStartedBy(a2,e,sAttr) hold. ";

   a prov:Activity;
   prov:wasStartedBy :a1;
.

# <= If and only if =>

:e 
   a prov:Entity;
   dcterms:description "wasGeneratedBy(e,a1,gAttr)";
   prov:wasGeneratedBy :a1;
.

:a1
   prov:generated :e;
   prov:qualfiedGeneration [
      a prov:Generation;
      prov:qualifiedEntity :e;
      :gAttr1 1;
      :gAttr2 2;
   ];
.

:a2
   dcterms:description "wasStartedBy(a2,e,sAttr)";
   prov:wasStartedBy :e;
.

:e
   prov:started :a2; # analogous to how prov:generation is used as the inverse of prov:wasGeneratedBy.
   prov:qualfiedStart [
      a prov:Start;
      prov:qualifiedActivity :a2;
      :sAttr1 3;
      :sAttr2 4;
   ];
.
