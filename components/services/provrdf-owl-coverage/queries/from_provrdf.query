prefix dcterms: <http://purl.org/dc/terms/>

select distinct ?construct min(?o) as ?yes
where {
   #graph <http://logd.tw.rpi.edu/source/w3-org/dataset/2011-prov-rdf-mappings/version/2012-Feb-29/meta> {
   #}
   graph <http://logd.tw.rpi.edu/source/w3-org/dataset/2011-prov-rdf-mappings/version/2012-Feb-29/provrdf> {
     [] dcterms:subject ?construct
   } 
   optional {
      graph <http://logd.tw.rpi.edu/source/w3-org/dataset/2011-prov-rdf-mappings/version/2012-Feb-29/owl> {
         {?construct a owl:Class}          union
         {?construct a owl:ObjectProperty} union
         {?construct a owl:DatatypeProperty}
         filter(regex(str(?construct),'http://www.w3.org/ns/prov.*'))
         ?construct ?p ?o
      }
   }
}

