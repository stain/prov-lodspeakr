prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix dcterms: <http://purl.org/dc/terms/>
prefix example: <http://example.org/ns#>
prefix prov:    <http://www.w3.org/ns/prov#>

select ?expression max(?type) as ?yes
where {
   graph <http://logd.tw.rpi.edu/source/w3-org/dataset/2011-prov-rdf-mappings/version/2012-Mar-01/provrdf> {
      [] a example:Expression;
         rdf:value        ?expression;
         prov:hadLocation ?order .
   }
   optional {
      graph <http://logd.tw.rpi.edu/source/w3-org/dataset/2011-prov-rdf-mappings/version/2012-Mar-01/provdm> {
         [] a ?type;
            rdf:value ?expression .
      }
   }
} order by ?order
