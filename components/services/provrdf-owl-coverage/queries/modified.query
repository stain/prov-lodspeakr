prefix pmlp: <http://inference-web.org/2.0/pml-provenance.owl#>

select distinct ?modified
where {
   graph <http://logd.tw.rpi.edu/source/w3-org/dataset/2011-prov-rdf-mappings/version/2012-Mar-01/meta> {
      [] a pmlp:SourceUsage;
         pmlp:hasSource       <https://dvcs.w3.org/hg/prov/raw-file/default/ontology/ProvenanceOntology.owl>;
         pmlp:hasUsageDateTime ?modified
      .
   }
}
